{
  "name": "cardnial-site",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "watch:sass": "sass --watch src:_site/css",
    "build:sass": "sass src:_site/css",
    "watch:eleventy": "eleventy --serve",
    "build:eleventy": "eleventy",
    "start": "npm-run-all build:sass --parallel watch:*",
    "build": "npm-run-all build:*"
  },
  "devDependencies": {
    "@11ty/eleventy": "^3.0.0",
    "npm-run-all": "^4.1.5",
    "sass": "^1.81.0"
  }
}
